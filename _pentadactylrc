map -builtin      n      j
map -builtin      N      J
map -builtin     gn     gj
map -builtin     gN     gJ
map -builtin     zn     zj
map -builtin <c-w>n <c-w>j
map -builtin <c-w>N <c-w>J

map -builtin      e      k
map -builtin      E      K
map -builtin     ge     gk
map -builtin     gE     gK|" nop
map -builtin     ze     zk
map -builtin <c-w>e <c-w>k
map -builtin <c-w>E <c-w>K

map -builtin      i      l
map -builtin      I      L
map -builtin     zi     zl
map -builtin     zI     zL
map -builtin <c-w>i <c-w>l
map -builtin <c-w>I <c-w>L
map -builtin     [i     [l|" nop
map -builtin     [I     [L|" nop
map -builtin     ]i     ]l|" nop
map -builtin     ]I     ]L|" nop

map -builtin      j      e
map -builtin      J      E
map -builtin     gj     ge
map -builtin     gJ     gE
map -builtin     zj     ze
map -builtin     zJ     zE

map -builtin      k      n
map -builtin      K      N
map -builtin     gk     gn|" nop
map -builtin     zk     zn
map -builtin     zK     zN
map -builtin <c-w>k <c-w>n

map -builtin      l      i
map -builtin      L      I
map -builtin     gl     gi
map -builtin     gL     gI
map -builtin     zl     zi
map -builtin <c-w>l <c-w>i
map -builtin     [l     [i
map -builtin     [L     [I
map -builtin     ]l     ]i
map -builtin     ]L     ]I

map <C-j> :downloads<cr>
